@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find required dependencies
find_dependency(Threads REQUIRED)

# Include the exported targets
include("${CMAKE_CURRENT_LIST_DIR}/namigatorTargets.cmake")

# Check that all required components are found
check_required_components(namigator)

# Provide namigator::namigator as an alias for backward compatibility
if(NOT TARGET namigator::namigator)
    add_library(namigator::namigator INTERFACE IMPORTED)
    set_target_properties(namigator::namigator PROPERTIES
        INTERFACE_LINK_LIBRARIES "namigator::pathfind;namigator::mapbuild"
    )
endif()